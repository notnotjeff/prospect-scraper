{
  "name": "prospect-scraper",
  "version": "0.1.0",
  "description": "Scrape NHL prospects from multiple development leagues",
  "keywords": [
    "nhl",
    "prospects",
    "draft",
    "hockey",
    "scraper"
  ],
  "homepage": "https://github.com/notnotjeff/prospect-scraper",
  "author": "Jeff Craig",
  "contributors": [
    "Jeff Craig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/notnotjeff/prospect-scraper.git"
  },
  "bugs": "https://github.com/notnotjeff/prospect-scraper/issues",
  "engines": {
    "node": "^14.9.0"
  },
  "main": "lib/prospectScraper.js",
  "jest": {
    "verbose": true,
    "modulePathIgnorePatterns": [
      "__fixtures__"
    ],
    "clearMocks": true
  },
  "scripts": {
    "test": "jest --watch",
    "test:no-watch": "jest",
    "open-coverage": "open coverage/lcov-report/index.html",
    "lint": "eslint --ignore-path ../.gitignore .",
    "prettier": "prettier --ignore-path ../.gitignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-format && npm run lint && npm run test"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "puppeteer": "^5.5.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "event-stream": "^4.0.1",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "prettier": "^2.0.5"
  },
  "license": "MIT"
}
