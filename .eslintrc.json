{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "globals": {
    "process": true,
    "__dirname": true
  },
  "rules": {
    "camelcase": "warn",
    "no-unused-vars": "warn"
  },
  "ignorePatterns": ["__fixtures__/", "*.md"]
}
